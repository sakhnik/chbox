[Exec]
Capability=CAP_IPC_LOCK
Boot=yes
PrivateUsers=no
Environment=DISPLAY=:0

[Files]
Bind=/home/sakhnik
Bind=/tmp/.X11-unix
Bind=/home/sakhnik/.Xauthority
Bind=/dev/dri
Bind=/dev/shm
Bind=/dev/input
#Bind=/run/user/1000/pulse:/run/user/host/pulse
Bind=/dev/snd
Bind=/dev/fuse
Bind=/media/sakhnik

[Network]
Private=no
